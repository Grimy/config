! vim: set ft=xdefaults
xterm*termName: xterm-256color

*loginShell:        true
*locale:            true
*bellIsUrgent:      true

*metaSendsEscape:   true
*backarrowKey:      true

*saveLines:         65535
*scrollTTYOutput:   true
*scrollTTYKeypress: true
xterm*scrollBar:    false

URxvt.iso14755:     false
URxvt.iso14755_52:  false

*borderWidth: 0
! *faceName: Droid Sans Mono for Powerline
*faceName: Liberation Mono for Powerline
*faceSize: 11
*scaleHeight: 1
*showBlinkAsBold: True

