#!/usr/bin/fish

function palette
	set i 0
	echo -ne "\e]4;"
	for color in $argv
		echo -ne "$i;#$color;"
		set i (expr $i + 1)
	end
	echo -ne "\007"
end

palette 073642 000000 719e07 000000 268bd2 000000 000000 eee8d5 \
		002b36 cb4b16 586e75 dc322f 839496 6c71c4 b58900 000000
~/.vim/src/build/bin/nvim -u ~/.vim/vimrc $argv
palette 000000 ff1230 4fd92e ffc36b 0a98e4 e428a3 09cf7f c1c5d3 \
		515151 ff5311 8de231 ffff9b 90e4ff 620a92 39ffaf ffffff
